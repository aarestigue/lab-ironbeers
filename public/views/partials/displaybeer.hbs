<div class="card">
    <div class="card-top">
        <img id="beer" src="{{this.image_url}}" alt="">
    </div>
    <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <div class="card-text"> {{this.description}} </div>
        <div class="card-line"> {{this.tagline}} </div>
    </div>
</div>